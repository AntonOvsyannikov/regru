	var opened = new Array();
	
	function show_menu_div(id, mode)
	{
		if(opened[id]=="closed")
		{
			//$(".tree_menu_v2_lvl3").hide();
			$(".childs"+id).show();
			opened[id]="opened";
			
			if(mode == "main")
			{
				$("#img"+id).attr({
          	src: "/bitrix/templates/store_minimal_red/images/to_close_folder.gif",
          	title: "–ó–∞–∫—Ä—ã—Ç—å –º–µ–Ω—é",
          	alt: "–ó–∞–∫—Ä—ã—Ç—å –º–µ–Ω—é"
        	});
			}
			
		}else
		{
			$(".childs"+id).hide();
			opened[id]="closed";
			if(mode == "main")
			{
				$(".tree_menu_v2_lvl3").hide();
				$("#img"+id).attr({
          	src: "/bitrix/templates/store_minimal_red/images/to_open_folder.gif",
          	title: "–ü–æ–∫–∞–∑–∞—Ç—å –º–µ–Ω—é",
          	alt: "–ü–æ–∫–∞–∑–∞—Ç—å –º–µ–Ω—é"
        	});
			}
			
		}
	}
	
</script>

